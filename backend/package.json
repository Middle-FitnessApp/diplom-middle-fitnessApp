{
	"name": "fitness-backend",
	"type": "module",
	"version": "1.0.0",
	"scripts": {
		"dev": "tsx --watch ./server.ts",
		"build": "tsc",
		"start": "node ./dist/server.js",
		"prisma:generate": "npx prisma generate",
		"prisma:migrate": "npx prisma migrate dev --name init",
		"prisma:migrate:name": "npx prisma migrate dev --name",
		"prisma:migrate:dev": "npx prisma migrate dev",
		"prisma:reset": "npx prisma migrate reset --force",
		"prisma:seed": "tsx ./prisma/seed.ts",
		"prisma:seed:light": "tsx ./prisma/seed-light.ts",
		"prisma:studio": "npx prisma studio",
		"test": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:folder": "npx vitest tests/$1",
		"test:unit": "npx vitest run tests/unit/",
		"test:integration": "npx vitest run tests/integration/",
		"test:e2e": "npx vitest run tests/e2e/"
	},
	"dependencies": {
		"@faker-js/faker": "10.1.0",
		"@fastify/cookie": "11.0.2",
		"@fastify/cors": "11.1.0",
		"@fastify/multipart": "9.3.0",
		"@fastify/sensible": "^6.0.4",
		"@fastify/static": "8.3.0",
		"@prisma/adapter-pg": "6.19.0",
		"@prisma/client": "6.19.0",
		"@supabase/supabase-js": "2.86.0",
		"bcrypt": "6.0.0",
		"bcryptjs": "3.0.3",
		"fastify": "5.6.2",
		"fastify-type-provider-zod": "6.1.0",
		"jsonwebtoken": "9.0.2",
		"socket.io": "4.7.5",
		"zod": "4.1.12"
	},
	"devDependencies": {
		"@types/bcrypt": "6.0.0",
		"@types/jsonwebtoken": "9.0.10",
		"@types/socket.io": "3.0.2",
		"@vitest/coverage-v8": "4.0.15",
		"dotenv": "17.2.3",
		"dotenv-cli": "11.0.0",
		"form-data": "4.0.5",
		"formdata-node": "6.0.3",
		"nodemon": "3.1.11",
		"prisma": "6.19.0",
		"ts-node": "10.9.2",
		"tsx": "4.19.3",
		"typescript": "5.9.3",
		"undici": "7.16.0",
		"vitest": "4.0.15"
	}
}
